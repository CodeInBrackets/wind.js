{
  "name": "@wind-js/wind.js",
  "version": "1.0.0",
  "main": "dist/wind.cjs.js",
  "description": "Reactive, actor based framework that can be used in client and server side.",
  "scripts": {
    "docs": "jsdoc2md lib/**/*.js > docs/reference.md",
    "build": "rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wind-js/wind.js.git"
  },
  "author": "Kevin Mas Ruiz <masruizkevin@gmail.com> (https://github.com/kmruiz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wind-js/wind.js/issues"
  },
  "contributors": [
    "Kevin Mas Ruiz <masruizkevin@gmail.com> (https://github.com/kmruiz)"
  ],
  "keywords": [
    "actors",
    "async",
    "wind.js",
    "front-end",
    "backend",
    "framework"
  ],
  "homepage": "https://github.com/wind-js/wind.js#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "jest": "^23.5.0",
    "jsdoc-to-markdown": "^4.0.1",
    "precommit-hook": "^3.0.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "uuid": "^3.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "test",
    "build",
    "docs"
  ]
